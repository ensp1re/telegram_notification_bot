{
  "name": "twitter-api",
  "version": "1.0.0",
  "description": "NestJS Twitter scraping API with account pool and proxy rotation",
  "main": "dist/main.js",
  "scripts": {
    "start": "ts-node -r reflect-metadata src/main.ts",
    "start:dev": "ts-node --watch -r reflect-metadata src/main.ts",
    "fetch-tweets": "tsx src/cli.ts",
    "test": "jest --forceExit --detectOpenHandles",
    "test:e2e": "jest --config jest-e2e.config.js --forceExit --detectOpenHandles"
  },
  "type": "commonjs",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/swagger": "^11.2.6",
    "@the-convocation/twitter-scraper": "^0.22.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "undici": "^7.22.0",
    "x-client-transaction-id": "^0.1.9"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.8.0",
    "@swc/core": "^1.15.11",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.2.3",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
